// Place your key bindings in this file to override the defaults
[
    {
        "key": "shift+alt+9",
        "command": "paredit.wrapAroundParens",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "shift+alt+0",
        "command": "paredit.wrapAroundParens",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "ctrl+shift+alt+9",
        "command": "-paredit.wrapAroundParens",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "alt+[",
        "command": "paredit.wrapAroundSquare",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "alt+]",
        "command": "paredit.wrapAroundSquare",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "ctrl+alt+[",
        "command": "-paredit.wrapAroundSquare",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "shift+alt+[",
        "command": "paredit.wrapAroundCurly",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "shift+alt+]",
        "command": "paredit.wrapAroundCurly",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "ctrl+shift+alt+[",
        "command": "-paredit.wrapAroundCurly",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "shift+alt+'",
        "command": "paredit.wrapAroundQuote",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "shift+l",
        "command": "paredit.forwardSexpOrUp",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode == 'Normal'"
    },
    {
        "key": "shift+l",
        "command": "paredit.selectForwardSexpOrUp",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode == 'Visual'"
    },
    {
        "key": "shift+h",
        "command": "paredit.backwardSexpOrUp",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode == 'Normal'"
    },
    {
        "key": "shift+h",
        "command": "paredit.selectBackwardSexpOrUp",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode == 'Visual'"
    },
    {
        "key": "shift+.",
        "command": "paredit.slurpSexpForward",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert' && !renameInputVisible"
    },
    {
        "key": "ctrl+alt+.",
        "command": "-paredit.slurpSexpForward",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "shift+,",
        "command": "paredit.barfSexpForward",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert' && !renameInputVisible"
    },
    {
        "key": "ctrl+alt+,",
        "command": "-paredit.barfSexpForward",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "shift+alt+,",
        "command": "paredit.slurpSexpBackward",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "ctrl+shift+alt+.",
        "command": "-paredit.slurpSexpBackward",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "shift+alt+.",
        "command": "paredit.barfSexpBackward",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "ctrl+shift+alt+,",
        "command": "-paredit.barfSexpBackward",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "alt+d",
        "command": "paredit.killSexpForward",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "alt-shift+r",
        "command": "paredit.raiseSexp",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "alt+j",
        "command": "paredit.dragSexprForward",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "alt+k",
        "command": "paredit.dragSexprBackward",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "backspace",
        "command": "paredit.deleteBackward",
        "when": "calva:keybindingsEnabled && editorTextFocus && !calva:cursorInComment && !editorHasMultipleSelections && !editorReadOnly && editorLangId == 'clojure' && paredit:keyMap == 'strict' && vim.active && vim.mode == 'Insert'"
    },
    {
        "key": "backspace",
        "command": "-paredit.deleteBackward",
        "when": "calva:keybindingsEnabled && editorTextFocus && !calva:cursorInComment && !editorHasMultipleSelections && !editorReadOnly && editorLangId == 'clojure' && paredit:keyMap == 'strict'"
    },
    {
        "key": "alt+l",
        "command": "settings.cycle.lineNumbers"
    },
    {
        "key": "alt+s",
        "command": "settings.cycle.statusBar"
    },
    {
        "key": "ctrl+j",
        "command": "selectNextSuggestion",
        "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus || suggestWidgetVisible && textInputFocus && !suggestWidgetHasFocusedSuggestion"
    },
    {
        "key": "ctrl+k",
        "command": "selectPrevSuggestion",
        "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus || suggestWidgetVisible && textInputFocus && !suggestWidgetHasFocusedSuggestion"
    },
    {
        "key": "ctrl+shift+b",
        "command": "bookmarksExplorer.focus"
    },
    {
        "key": "alt+a",
        "command": "-editor.action.accessibilityHelpConfigureAssignedKeybindings",
        "when": "accessibilityHelpIsShown && accessibleViewHasAssignedKeybindings"
    },
    {
        "key": "alt+a",
        "command": "settings.cycle.activityBar"
    },
    {
        "key": "alt+m",
        "command": "settings.cycle.glyphMargin"
    },
    {
        "key": "alt+t",
        "command": "workbench.view.testing.focus"
    },
    {
        "key": "alt+cmd+r",
        "command": "vscode-hurl-runner.runHurl",
        "when": "editorTextFocus && editorLangId == 'hurl'"
    },
    {
        "key": "alt+cmd+h",
        "command": "-vscode-hurl-runner.runHurl",
        "when": "editorTextFocus && editorLangId == 'hurl'"
    },
    {
        "key": "ctrl+x d",
        "command": "oil-code.open"
    },
    {
        "key": "alt+-",
        "command": "-oil-code.open"
    },
    {
        "key": "ctrl+shift+d",
        "command": "github.cweijan.mysql.focus"
    },
    {
        "key": "ctrl+shift+d",
        "command": "pg-objectExplorer.focus"
    },
    {
        "key": "k",
        "command": "-magit.discard-at-point",
        "when": "editorTextFocus && editorLangId == 'magit' && vim.mode =~ /^(?!SearchInProgressMode|CommandlineInProgress).*$/"
    },
    {
        "key": "x",
        "command": "-magit.reset-mixed",
        "when": "editorTextFocus && editorLangId == 'magit' && vim.mode =~ /^(?!SearchInProgressMode|CommandlineInProgress).*$/"
    },
    {
        "key": "shift+k",
        "command": "magit.discard-at-point",
        "when": "editorTextFocus && editorLangId == 'magit'"
    },
    {
        "key": "tab",
        "command": "magit.toggle-fold",
        "when": "editorTextFocus && editorLangId == 'magit'"
    },
    {
        "key": "tab",
        "command": "-magit.toggle-fold",
        "when": "editorTextFocus && editorLangId == 'magit' && vim.mode =~ /^(?!SearchInProgressMode|CommandlineInProgress).*$/"
    },
    {
        "key": "ctrl+j",
        "command": "workbench.action.quickOpenSelectNext",
        "when": "inQuickOpen"
    },
    {
        "key": "ctrl+n",
        "command": "-workbench.action.quickOpenSelectNext",
        "when": "inQuickOpen"
    },
    {
        "key": "ctrl+k",
        "command": "workbench.action.quickOpenSelectPrevious",
        "when": "inQuickOpen"
    },
    {
        "key": "ctrl+p",
        "command": "-workbench.action.quickOpenSelectPrevious",
        "when": "inQuickOpen"
    }
]