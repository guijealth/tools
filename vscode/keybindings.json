// Place your key bindings in this file to override the defaults
[
    {
        "key": "shift+alt+9",
        "command": "paredit.wrapAroundParens",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "shift+alt+0",
        "command": "paredit.wrapAroundParens",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "ctrl+shift+alt+9",
        "command": "-paredit.wrapAroundParens",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "alt+[",
        "command": "paredit.wrapAroundSquare",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "alt+]",
        "command": "paredit.wrapAroundSquare",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "ctrl+alt+[",
        "command": "-paredit.wrapAroundSquare",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "shift+alt+[",
        "command": "paredit.wrapAroundCurly",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "shift+alt+]",
        "command": "paredit.wrapAroundCurly",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "ctrl+shift+alt+[",
        "command": "-paredit.wrapAroundCurly",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "shift+alt+'",
        "command": "paredit.wrapAroundQuote",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "shift+l",
        "command": "paredit.forwardSexpOrUp",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode == 'Normal'"
    },
    {
        "key": "shift+l",
        "command": "paredit.selectForwardSexpOrUp",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode == 'Visual'"
    },
    {
        "key": "shift+h",
        "command": "paredit.backwardSexpOrUp",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode == 'Normal'"
    },
    {
        "key": "shift+h",
        "command": "paredit.selectBackwardSexpOrUp",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode == 'Visual'"
    },
    {
        "key": "shift+.",
        "command": "paredit.slurpSexpForward",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert' && !renameInputVisible"
    },
    {
        "key": "ctrl+alt+.",
        "command": "-paredit.slurpSexpForward",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "shift+,",
        "command": "paredit.barfSexpForward",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert' && !renameInputVisible"
    },
    {
        "key": "ctrl+alt+,",
        "command": "-paredit.barfSexpForward",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "shift+alt+,",
        "command": "paredit.slurpSexpBackward",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "ctrl+shift+alt+.",
        "command": "-paredit.slurpSexpBackward",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "shift+alt+.",
        "command": "paredit.barfSexpBackward",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "ctrl+shift+alt+,",
        "command": "-paredit.barfSexpBackward",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "alt+d",
        "command": "paredit.killSexpForward",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "alt-shift+r",
        "command": "paredit.raiseSexp",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "alt+j",
        "command": "paredit.dragSexprForward",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "alt+k",
        "command": "paredit.dragSexprBackward",
        "when": "editorLangId == 'clojure' && vim.active && vim.mode != 'Insert'"
    },
    {
        "key": "backspace",
        "command": "paredit.deleteBackward",
        "when": "calva:keybindingsEnabled && editorTextFocus && !calva:cursorInComment && !editorHasMultipleSelections && !editorReadOnly && editorLangId == 'clojure' && paredit:keyMap == 'strict' && vim.active && vim.mode == 'Insert'"
    },
    {
        "key": "backspace",
        "command": "-paredit.deleteBackward",
        "when": "calva:keybindingsEnabled && editorTextFocus && !calva:cursorInComment && !editorHasMultipleSelections && !editorReadOnly && editorLangId == 'clojure' && paredit:keyMap == 'strict'"
    }
]