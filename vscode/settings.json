{
    "workbench.iconTheme": "vscode-icons",
    "window.zoomLevel": 1,
    "redhat.telemetry.enabled": false,
    "editor.accessibilitySupport": "off",
    "editor.unicodeHighlight.invisibleCharacters": false,
    "security.workspace.trust.untrustedFiles": "open",
    "csv-edit.readOption_hasHeader": "true",
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    },
    "editor.largeFileOptimizations": false,
    "calva.prettyPrintingOptions": {
        "printEngine": "pprint",
        "enabled": false,
        "width": 120,
        "maxLength": 50
    },
    "calva.autoOpenInspector": false,
    "editor.minimap.size": "fit",
    "editor.minimap.maxColumn": 120,
    "calva.useTestExplorer": true,
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "github.copilot.editor.enableAutoCompletions": false,
    "calva.paredit.defaultKeyMap": "strict",
    "editor.renderWhitespace": "none",
    "calva.paredit.killAlsoCutsToClipboard": true,
    "bookmarks.keepBookmarksOnLineDelete": true,
    "codesnap.containerPadding": "1em",
    "codesnap.showWindowControls": false,
    "codesnap.target": "window",
    "codesnap.transparentBackground": true,
    "editor.renderControlCharacters": false,
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "calva.autoEvaluateCode": {
        "onConnect": {
            "clj": "(when-let [requires (resolve 'clojure.main/repl-requires)] (clojure.core/apply clojure.core/require @requires))",
            "cljs": "(require '[cljs.repl :refer [apropos dir doc find-doc print-doc pst source]])"
        },
        "onFileLoaded": {
            "clj": null,
            "cljs": null
        }
    },
    "github.copilot.enable": {
        "*": false,
        "plaintext": false,
        "markdown": false,
        "scminput": false
    },
    "calva.outputDestinations": {
        "evalResults": "repl-window",
        "evalOutput": "repl-window",
        "otherOutput": "repl-window"
    },
    "editor.minimap.enabled": false,
    "files.autoSave": "off",
    "docker.extension.enableComposeLanguageServer": false,
    "workbench.activityBar.location": "hidden",
    "workbench.statusBar.visible": true,
    "workbench.sideBar.location": "right",
    "editor.hideCursorInOverviewRuler": true,
    "editor.overviewRulerBorder": false,
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.vertical": "hidden",
    "breadcrumbs.enabled": false,
    "workbench.tree.renderIndentGuides": "none",
    "explorer.decorations.colors": false,
    "editor.guides.indentation": false,
    "editor.cursorBlinking": "phase",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.renderLineHighlight": "none",
    "editor.glyphMargin": false,
    "editor.lineNumbers": "off",
    "editor.showFoldingControls": "mouseover",
    "workbench.layoutControl.type": "menu",
    "window.title": "${rootName}${separator}${profileName}",
    "workbench.editor.showTabs": "single",
    "editor.stickyScroll.maxLineCount": 10,
    "settings.cycle": [
        {
            "id": "lineNumbers",
            "values": [
                {
                    "editor.lineNumbers": "off"
                },
                {
                    "editor.lineNumbers": "on"
                }
            ]
        },
        {
            "id": "statusBar",
            "values": [
                {
                    "workbench.statusBar.visible": false
                },
                {
                    "workbench.statusBar.visible": true
                }
            ]
        },
        {
            "id": "activityBar",
            "values": [
                {
                    "workbench.activityBar.location": "hidden",
                },
                {
                    "workbench.activityBar.location": "default",
                }
            ]
        },
        {
            "id": "glyphMargin",
            "values": [
                {
                    "editor.glyphMargin": false,
                },
                {
                    "editor.glyphMargin": true,
                }
            ]
        }
    ],
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "workbench.colorTheme": "Doom Emacs",
    "diffEditor.renderSideBySide": true,
    "window.commandCenter": false,
    "workbench.secondarySideBar.defaultVisibility": "hidden",
    "makefile.configureOnOpen": true,
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "window.confirmBeforeClose": "always",
    "[dockercompose]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": "advanced",
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": true
        },
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "[github-actions-workflow]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "vscode-hurl-runner.verboseMode": "very-verbose",
    "vim.leader": "<space>",
    "vim.useSystemClipboard": true,
    "vim.visualModeKeyBindings": [
        {
            "before": [
                "leader",
                "m",
                "e",
                "r"
            ],
            "when": "calva:keybindingsEnabled && editorTextFocus && !suggestWidgetVisible && editorLangId == 'clojure'",
            "commands": [
                {
                    "command": "calva.evaluateSelection"
                }
            ]
        }
    ],
    "vim.normalModeKeyBindings": [
        // project
        {
            "before": [
                "leader",
                "p",
                "p"
            ],
            "commands": [
                {
                    "command": "projectManager.listProjects"
                }
            ]
        },
        // windows
        {
            "before": [
                "leader",
                "w",
                "v"
            ],
            "commands": [
                {
                    "command": "workbench.action.splitEditor"
                }
            ]
        },
        {
            "before": [
                "leader",
                "w",
                "s"
            ],
            "commands": [
                {
                    "command": "workbench.action.splitEditorDown"
                }
            ]
        },
        {
            "before": [
                "leader",
                "w",
                "q"
            ],
            "commands": [
                {
                    "command": "workbench.action.closeActiveEditor"
                }
            ]
        },
        {
            "before": [
                "leader",
                "b",
                "k"
            ],
            "commands": [
                {
                    "command": "workbench.action.closeActiveEditor"
                }
            ]
        },
        {
            "before": [
                "leader",
                "w",
                "+"
            ],
            "commands": [
                {
                    "command": "workbench.action.increaseViewWidth"
                }
            ]
        },
        {
            "before": [
                "leader",
                "w",
                "-"
            ],
            "commands": [
                {
                    "command": "workbench.action.decreaseViewWidth"
                }
            ]
        },
        {
            "before": [
                "leader",
                "w",
                "h"
            ],
            "commands": [
                {
                    "command": "workbench.action.navigateLeft"
                }
            ]
        },
        {
            "before": [
                "leader",
                "w",
                "l"
            ],
            "commands": [
                {
                    "command": "workbench.action.navigateRight"
                }
            ]
        },
        {
            "before": [
                "leader",
                "w",
                "j"
            ],
            "commands": [
                {
                    "command": "workbench.action.navigateDown"
                }
            ]
        },
        {
            "before": [
                "leader",
                "w",
                "k"
            ],
            "commands": [
                {
                    "command": "workbench.action.navigateUp"
                }
            ]
        },
        // terminal
        {
            "before": [
                "leader",
                "o",
                "t"
            ],
            "commands": [
                {
                    "command": "workbench.action.terminal.toggleTerminal"
                }
            ]
        },
        // explorer
        {
            "before": [
                "leader",
                "o",
                "p"
            ],
            "commands": [
                {
                    "command": "workbench.view.explorer"
                }
            ]
        },
        {
            "before": [
                "leader",
                "o",
                "p"
            ],
            "when": "filesExplorerFocus",
            "commands": [
                {
                    "command": "workbench.action.toggleSidebarVisibility"
                }
            ]
        },
        // fold
        {
            "before": [
                "z",
                "a"
            ],
            "when": "editorTextFocus && foldingEnabled",
            "commands": [
                {
                    "command": "editor.toggleFold"
                }
            ]
        },
        // dired
        {
            "before": [
                "<c-x>",
                "d"
            ],
            "commands": [
                {
                    "command": "oil-code.open"
                },
                {
                    "command": "oil-code.preview"
                }
            ]
        },
        {
            "before": [
                "<esc>"
            ],
            "when": "editorTextFocus && editorLangId == 'oil'",
            "commands": [
                "oil-code.close"
            ]
        },
        {
            "before": [
                "<cr>"
            ],
            "commands": [
                {
                    "command": "oil-code.select"
                }
            ]
        },
        {
            "before": [
                "<c-l>"
            ],
            "commands": [
                {
                    "command": "oil-code.refresh"
                }
            ]
        },
        {
            "before": [
                "<a-`>"
            ],
            "when": "editorTextFocus && editorLangId == 'oil'",
            "commands": [
                {
                    "command": "oil-code.cd"
                }
            ]
        },
        {
            "before": [
                "-"
            ],
            "when": "editorTextFocus && editorLangId == 'oil'",
            "commands": [
                "oil-code.open"
            ]
        },
        // calva
        {
            "before": [
                "leader",
                "m",
                "c"
            ],
            "when": "workspaceFolderCount > 0",
            "commands": [
                {
                    "command": "calva.connect"
                }
            ]
        },
        {
            "before": [
                "leader",
                "o",
                "r"
            ],
            "when": "calva:connected",
            "commands": [
                {
                    "command": "calva.showReplWindow"
                }
            ]
        },
        {
            "before": [
                "leader",
                "m",
                "e",
                "b"
            ],
            "when": "calva:keybindingsEnabled && editorLangId == 'clojure'",
            "commands": [
                {
                    "command": "calva.loadFile"
                }
            ]
        },
        {
            "before": [
                "leader",
                "m",
                "e",
                "d"
            ],
            "when": "calva:keybindingsEnabled && editorTextFocus && editorLangId == 'clojure'",
            "commands": [
                {
                    "command": "calva.evaluateCurrentTopLevelForm"
                }
            ]
        },
        {
            "before": [
                "leader",
                "m",
                "e",
                "e"
            ],
            "when": "calva:keybindingsEnabled && editorTextFocus && !suggestWidgetVisible && editorLangId == 'clojure'",
            "commands": [
                {
                    "command": "calva.evaluateSelection"
                }
            ]
        },
        {
            "before": [
                "leader",
                "m",
                "e",
                "E"
            ],
            "when": "calva:connected",
            "commands": [
                {
                    "command": "calva.sendCurrentFormToOutputWindow"
                }
            ]
        },
        {
            "before": [
                "leader",
                "m",
                "e",
                "c"
            ],
            "when": "calva:keybindingsEnabled && editorTextFocus && editorLangId == 'clojure'",
            "commands": [
                {
                    "command": "calva.evaluateSelectionAsComment"
                }
            ]
        },
        {
            "before": [
                "leader",
                "m",
                "r",
                "r"
            ],
            "when": "calva:connected",
            "commands": [
                {
                    "command": "calva.refreshAll"
                }
            ]
        },
        {
            "before": [
                "leader",
                "m",
                "t",
                "p"
            ],
            "when": "calva:connected",
            "commands": [
                {
                    "command": "calva.runAllTests"
                }
            ]
        },
        {
            "before": [
                "leader",
                "m",
                "t",
                "n"
            ],
            "when": "calva:connected",
            "commands": [
                {
                    "command": "calva.runNamespaceTests"
                }
            ]
        },
        {
            "before": [
                "leader",
                "m",
                "t",
                "n"
            ],
            "when": "calva:connected",
            "commands": [
                {
                    "command": "calva.runNamespaceTests"
                }
            ]
        },
        {
            "before": [
                "leader",
                "m",
                "t",
                "t"
            ],
            "when": "calva:connected",
            "commands": [
                {
                    "command": "calva.runTestUnderCursor"
                }
            ]
        }
    ],
    "oil-code.hasNerdFont": true,
    "editor.fontFamily": "FiraCode Nerd Font, Menlo, Monaco, 'Courier New', monospace",
    "oil-code.enableWorkspaceEdit": true,
    "oil-code.enableAlternateConfirmation": true,
    "prettier.enableDebugLogs": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "notebook.defaultFormatter": "esbenp.prettier-vscode",
    "projectManager.projectsLocation": "~/Work",
    "projectManager.sortList": "Recent",
    "projectManager.git.baseFolders": [
        "~/Work"
    ],
    "gitlens.ai.model": "vscode",
    "gitlens.ai.vscode.model": "copilot:gpt-4.1",
    "[clojure]": {
        "editor.defaultFormatter": "betterthantomorrow.calva"
    }
}