{
    "workbench.iconTheme": "vscode-icons",
    "window.zoomLevel": 1,
    "redhat.telemetry.enabled": false,
    "editor.accessibilitySupport": "off",
    "vim.useSystemClipboard": true,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "security.workspace.trust.untrustedFiles": "open",
    "csv-edit.readOption_hasHeader": "true",
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    },
    "editor.largeFileOptimizations": false,
    "calva.prettyPrintingOptions": {
        "printEngine": "pprint",
        "enabled": false,
        "width": 120,
        "maxLength": 50
    },
    "calva.autoOpenInspector": false,
    "editor.minimap.size": "fit",
    "editor.minimap.maxColumn": 120,
    "calva.useTestExplorer": true,
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "github.copilot.editor.enableAutoCompletions": false,
    "calva.paredit.defaultKeyMap": "strict",
    "editor.renderWhitespace": "none",
    "calva.paredit.killAlsoCutsToClipboard": true,
    "bookmarks.keepBookmarksOnLineDelete": true,
    "codesnap.containerPadding": "1em",
    "codesnap.showWindowControls": false,
    "codesnap.target": "window",
    "codesnap.transparentBackground": true,
    "editor.renderControlCharacters": false,
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "calva.autoEvaluateCode": {
        "onConnect": {
            "clj": "(when-let [requires (resolve 'clojure.main/repl-requires)] (clojure.core/apply clojure.core/require @requires))",
            "cljs": "(require '[cljs.repl :refer [apropos dir doc find-doc print-doc pst source]])"
        },
        "onFileLoaded": {
            "clj": null,
            "cljs": null
        }
    },
    "github.copilot.enable": {
        "*": false,
        "plaintext": false,
        "markdown": false,
        "scminput": false
    },
    "calva.outputDestinations": {
        "evalResults": "repl-window",
        "evalOutput": "repl-window",
        "otherOutput": "repl-window"
    },
    "editor.minimap.enabled": false,
    "files.autoSave": "off",
    "docker.extension.enableComposeLanguageServer": false,
    "workbench.activityBar.location": "hidden",
    "workbench.statusBar.visible": false,
    "workbench.sideBar.location": "right",
    "editor.hideCursorInOverviewRuler": true,
    "editor.overviewRulerBorder": false,
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.vertical": "hidden",
    "breadcrumbs.enabled": false,
    "workbench.tree.renderIndentGuides": "none",
    "explorer.decorations.colors": false,
    "editor.guides.indentation": false,
    "editor.cursorBlinking": "phase",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.renderLineHighlight": "none",
    "editor.glyphMargin": false,
    "editor.lineNumbers": "off",
    "editor.showFoldingControls": "mouseover",
    "workbench.layoutControl.type": "menu",
    "window.title": "${rootName}${separator}${profileName}",
    "workbench.editor.showTabs": "single",
    "editor.stickyScroll.maxLineCount": 10,
    "settings.cycle": [
        {
            "id": "lineNumbers",
            "values": [
                {
                    "editor.lineNumbers": "off"
                },
                {
                    "editor.lineNumbers": "on"
                }
            ]
        },
        {
            "id": "statusBar",
            "values": [
                {
                    "workbench.statusBar.visible": false
                },
                {
                    "workbench.statusBar.visible": true
                }
            ]
        },
        {
            "id": "activityBar",
            "values": [
                {
                    "workbench.activityBar.location": "hidden",
                },
                {
                    "workbench.activityBar.location": "default",
                }
            ]
        },
        {
            "id": "glyphMargin",
            "values": [
                {
                    "editor.glyphMargin": false,
                },
                {
                    "editor.glyphMargin": true,
                }
            ]
        }
    ],
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "workbench.colorTheme": "Doom Emacs",
    "diffEditor.renderSideBySide": true,
    "window.commandCenter": false,
    "workbench.secondarySideBar.defaultVisibility": "hidden",
    "makefile.configureOnOpen": true,
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "window.confirmBeforeClose": "always",
    "[dockercompose]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": "advanced",
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": true
        },
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "[github-actions-workflow]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "vscode-hurl-runner.verboseMode": "very-verbose",
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "-"
            ],
            "commands": [
                {
                    "command": "oil-code.open"
                }
            ]
        },
        {
            "before": [
                "<cr>"
            ],
            "commands": [
                {
                    "command": "oil-code.select"
                }
            ]
        },
        {
            "before": [
                "<c-l>"
            ],
            "commands": [
                {
                    "command": "oil-code.refresh"
                }
            ]
        },
        {
            "before": [
                "`"
            ],
            "commands": [
                {
                    "command": "oil-code.cd"
                }
            ]
        }
    ],
    "oil-code.hasNerdFont": true,
    "editor.fontFamily": "FiraCode Nerd Font, Menlo, Monaco, 'Courier New', monospace",
    "oil-code.enableWorkspaceEdit": true,
    "oil-code.enableAlternateConfirmation": true
}