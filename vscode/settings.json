{
    "[clojure]": {
        "editor.defaultFormatter": "betterthantomorrow.calva"
    },
    "[dockercompose]": {
        "editor.autoIndent": "advanced",
        "editor.defaultFormatter": "redhat.vscode-yaml",
        "editor.insertSpaces": true,
        "editor.quickSuggestions": {
            "comments": false,
            "other": true,
            "strings": true
        },
        "editor.tabSize": 2
    },
    "[github-actions-workflow]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    },
    "[sql]": {
        "editor.defaultFormatter": "ms-ossdata.vscode-pgsql"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "bookmarks.keepBookmarksOnLineDelete": true,
    "breadcrumbs.enabled": false,
    "calva.autoEvaluateCode": {
        "onConnect": {
            "clj": "(when-let [requires (resolve 'clojure.main/repl-requires)] (clojure.core/apply clojure.core/require @requires))",
            "cljs": "(require '[cljs.repl :refer [apropos dir doc find-doc print-doc pst source]])"
        },
        "onFileLoaded": {
            "clj": null,
            "cljs": null
        }
    },
    "calva.autoOpenInspector": false,
    "calva.outputDestinations": {
        "evalOutput": "repl-window",
        "evalResults": "repl-window",
        "otherOutput": "repl-window"
    },
    "calva.paredit.defaultKeyMap": "strict",
    "calva.paredit.killAlsoCutsToClipboard": true,
    "calva.prettyPrintingOptions": {
        "enabled": true,
        "maxLength": 50,
        "printEngine": "pprint",
        "width": 60
    },
    "calva.statusColor": {
        "dark": {
            "connectedStatusColor": "#9B9C9F",
            "disconnectedColor": "#9B9C9F",
            "launchingColor": "#9B9C9F",
            "typeStatusColor": "#9B9C9F"
        },
        "light": {
            "connectedStatusColor": "#DB9550",
            "disconnectedColor": "#777777",
            "launchingColor": "#000000",
            "typeStatusColor": "#91dc47"
        }
    },
    "calva.useTestExplorer": true,
    "codesnap.containerPadding": "1em",
    "codesnap.showWindowControls": false,
    "codesnap.target": "window",
    "codesnap.transparentBackground": true,
    "csv-edit.readOption_hasHeader": "true",
    "diffEditor.renderSideBySide": true,
    "docker.extension.enableComposeLanguageServer": false,
    "editor.accessibilitySupport": "off",
    "editor.cursorBlinking": "phase",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.fontFamily": "FiraCode Nerd Font, Menlo, Monaco, 'Courier New', monospace",
    "editor.glyphMargin": false,
    "editor.guides.indentation": false,
    "editor.hideCursorInOverviewRuler": true,
    "editor.largeFileOptimizations": true,
    "editor.lineNumbers": "off",
    "editor.minimap.enabled": false,
    "editor.minimap.maxColumn": 120,
    "editor.minimap.size": "fit",
    "editor.overviewRulerBorder": false,
    "editor.renderControlCharacters": false,
    "editor.renderLineHighlight": "line",
    "editor.renderWhitespace": "none",
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.vertical": "hidden",
    "editor.showFoldingControls": "mouseover",
    "editor.stickyScroll.maxLineCount": 10,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "explorer.decorations.colors": false,
    "files.autoSave": "afterDelay",
    "git.openRepositoryInParentFolders": "always",
    "github.copilot.editor.enableAutoCompletions": false,
    "github.copilot.enable": {
        "*": false,
        "markdown": false,
        "plaintext": false,
        "scminput": false
    },
    "gitlens.ai.model": "vscode",
    "gitlens.ai.vscode.model": "copilot:gpt-4.1",
    "makefile.configureOnOpen": true,
    "notebook.defaultFormatter": "esbenp.prettier-vscode",
    "oil-code.enableAlternateConfirmation": true,
    "oil-code.enableWorkspaceEdit": true,
    "oil-code.hasNerdFont": true,
    "pgsql.connections": [
        {
            "applicationName": "vscode-pgsql",
            "authenticationType": "SqlLogin",
            "clientEncoding": "utf8",
            "connectTimeout": 15,
            "expiresOn": 0,
            "groupId": "0E6C2A27-4345-42FF-A27E-3CA4BDA7283D",
            "id": "548CC561-735A-4110-8B6D-0582A43CCE89",
            "password": "",
            "port": "54321",
            "profileName": "tx1-poc",
            "savePassword": true,
            "server": "localhost",
            "sshPassword": "",
            "sslmode": "prefer",
            "user": "postgres"
        },
        {
            "applicationName": "vscode-pgsql",
            "authenticationType": "SqlLogin",
            "clientEncoding": "utf8",
            "connectTimeout": 15,
            "database": "aidbox",
            "expiresOn": 0,
            "groupId": "0E6C2A27-4345-42FF-A27E-3CA4BDA7283D",
            "id": "E6389B24-1C99-4689-AA48-FBFFF503E845",
            "password": "",
            "port": "5432",
            "profileName": "localhost, <default> (postgres)",
            "savePassword": true,
            "server": "localhost",
            "sshPassword": "",
            "sslmode": "prefer",
            "user": "aidbox"
        },
        {
            "applicationName": "vscode-pgsql",
            "authenticationType": "SqlLogin",
            "clientEncoding": "utf8",
            "connectTimeout": 15,
            "database": "testbox",
            "expiresOn": 0,
            "groupId": "0E6C2A27-4345-42FF-A27E-3CA4BDA7283D",
            "id": "7F35731D-DB1F-492F-96ED-6980D876FB4E",
            "password": "",
            "port": "5436",
            "profileName": "localhost:5432, testbox (postgres)",
            "savePassword": true,
            "server": "localhost",
            "sshPassword": "",
            "sslmode": "prefer",
            "user": "postgres"
        }
    ],
    "pgsql.serverGroups": [
        {
            "id": "0E6C2A27-4345-42FF-A27E-3CA4BDA7283D",
            "isDefault": true,
            "name": "Servers"
        }
    ],
    "prettier.enableDebugLogs": true,
    "projectManager.git.baseFolders": [
        "~/Work"
    ],
    "projectManager.projectsLocation": "~/Work",
    "projectManager.sortList": "Recent",
    "redhat.telemetry.enabled": false,
    "security.workspace.trust.untrustedFiles": "open",
    "settings.cycle": [
        {
            "id": "lineNumbers",
            "values": [
                {
                    "editor.lineNumbers": "off"
                },
                {
                    "editor.lineNumbers": "on"
                }
            ]
        },
        {
            "id": "statusBar",
            "values": [
                {
                    "workbench.statusBar.visible": false
                },
                {
                    "workbench.statusBar.visible": true
                }
            ]
        },
        {
            "id": "activityBar",
            "values": [
                {
                    "workbench.activityBar.location": "hidden"
                },
                {
                    "workbench.activityBar.location": "default"
                }
            ]
        },
        {
            "id": "glyphMargin",
            "values": [
                {
                    "editor.glyphMargin": false
                },
                {
                    "editor.glyphMargin": true
                }
            ]
        }
    ],
    "vim.easymotion": true,
    "vim.leader": "<space>",
    "vim.normalModeKeyBindings": [
        // project
        {
            "before": [
                "leader",
                "p",
                "p"
            ],
            "commands": [
                {
                    "command": "projectManager.listProjects"
                }
            ]
        },
        // windows
        {
            "before": [
                "leader",
                "w",
                "v"
            ],
            "commands": [
                {
                    "command": "workbench.action.splitEditor"
                }
            ]
        },
        {
            "before": [
                "leader",
                "w",
                "s"
            ],
            "commands": [
                {
                    "command": "workbench.action.splitEditorDown"
                }
            ]
        },
        {
            "before": [
                "leader",
                "w",
                "q"
            ],
            "commands": [
                {
                    "command": "workbench.action.closeActiveEditor"
                }
            ]
        },
        {
            "before": [
                "leader",
                "b",
                "k"
            ],
            "commands": [
                {
                    "command": "workbench.action.closeActiveEditor"
                }
            ]
        },
        {
            "before": [
                "leader",
                "b",
                "d"
            ],
            "commands": [
                {
                    "command": "workbench.action.closeActiveEditor"
                }
            ]
        },
        {
            "before": [
                "leader",
                "w",
                "+"
            ],
            "commands": [
                {
                    "command": "workbench.action.increaseViewWidth"
                }
            ]
        },
        {
            "before": [
                "leader",
                "w",
                "-"
            ],
            "commands": [
                {
                    "command": "workbench.action.decreaseViewWidth"
                }
            ]
        },
        {
            "before": [
                "leader",
                "w",
                "h"
            ],
            "commands": [
                {
                    "command": "workbench.action.navigateLeft"
                }
            ]
        },
        {
            "before": [
                "leader",
                "w",
                "l"
            ],
            "commands": [
                {
                    "command": "workbench.action.navigateRight"
                }
            ]
        },
        {
            "before": [
                "leader",
                "w",
                "j"
            ],
            "commands": [
                {
                    "command": "workbench.action.navigateDown"
                }
            ]
        },
        {
            "before": [
                "leader",
                "w",
                "k"
            ],
            "commands": [
                {
                    "command": "workbench.action.navigateUp"
                }
            ]
        },
        // terminal
        {
            "before": [
                "leader",
                "o",
                "t"
            ],
            "commands": [
                {
                    "command": "workbench.action.terminal.toggleTerminal"
                }
            ]
        },
        // explorer
        {
            "before": [
                "leader",
                "o",
                "p"
            ],
            "commands": [
                {
                    "command": "workbench.view.explorer"
                }
            ]
        },
        {
            "before": [
                "leader",
                "o",
                "p"
            ],
            "commands": [
                {
                    "command": "workbench.action.toggleSidebarVisibility"
                }
            ],
            "when": "filesExplorerFocus"
        },
        // fold
        {
            "before": [
                "z",
                "a"
            ],
            "commands": [
                {
                    "command": "editor.toggleFold"
                }
            ],
            "when": "editorTextFocus && foldingEnabled"
        },
        // dired
        {
            "before": [
                "<c-x>",
                "d"
            ],
            "commands": [
                {
                    "command": "oil-code.open"
                },
                {
                    "command": "oil-code.preview"
                }
            ]
        },
        {
            "before": [
                "<esc>"
            ],
            "commands": [
                "oil-code.close"
            ],
            "when": "editorTextFocus && editorLangId == 'oil'"
        },
        {
            "before": [
                "<cr>"
            ],
            "commands": [
                {
                    "command": "oil-code.select"
                }
            ]
        },
        {
            "before": [
                "<c-l>"
            ],
            "commands": [
                {
                    "command": "oil-code.refresh"
                }
            ]
        },
        {
            "before": [
                "<a-`>"
            ],
            "commands": [
                {
                    "command": "oil-code.cd"
                }
            ],
            "when": "editorTextFocus && editorLangId == 'oil'"
        },
        {
            "before": [
                "-"
            ],
            "commands": [
                "oil-code.open"
            ],
            "when": "editorTextFocus && editorLangId == 'oil'"
        },
        // calva
        {
            "before": [
                "leader",
                "m",
                "c"
            ],
            "commands": [
                {
                    "command": "calva.connect"
                }
            ],
            "when": "workspaceFolderCount > 0"
        },
        {
            "before": [
                "leader",
                "o",
                "r"
            ],
            "commands": [
                {
                    "command": "calva.showReplWindow"
                }
            ],
            "when": "calva:connected"
        },
        {
            "before": [
                "leader",
                "m",
                "e",
                "b"
            ],
            "commands": [
                {
                    "command": "calva.loadFile"
                }
            ],
            "when": "calva:keybindingsEnabled && editorLangId == 'clojure'"
        },
        {
            "before": [
                "leader",
                "m",
                "e",
                "d"
            ],
            "commands": [
                {
                    "command": "calva.evaluateCurrentTopLevelForm"
                }
            ],
            "when": "calva:keybindingsEnabled && editorTextFocus && editorLangId == 'clojure'"
        },
        {
            "before": [
                "leader",
                "m",
                "e",
                "e"
            ],
            "commands": [
                {
                    "command": "calva.evaluateSelection"
                }
            ],
            "when": "calva:keybindingsEnabled && editorTextFocus && !suggestWidgetVisible && editorLangId == 'clojure'"
        },
        {
            "before": [
                "leader",
                "m",
                "e",
                "E"
            ],
            "commands": [
                {
                    "command": "calva.sendCurrentFormToOutputWindow"
                }
            ],
            "when": "calva:connected"
        },
        {
            "before": [
                "leader",
                "m",
                "e",
                "c"
            ],
            "commands": [
                {
                    "command": "calva.evaluateSelectionAsComment"
                }
            ],
            "when": "calva:keybindingsEnabled && editorTextFocus && editorLangId == 'clojure'"
        },
        {
            "before": [
                "leader",
                "m",
                "p",
                "P"
            ],
            "commands": [
                {
                    "command": "calva.evaluateSelectionAsComment"
                }
            ],
            "when": "calva:keybindingsEnabled && editorTextFocus && editorLangId == 'clojure'"
        },
        {
            "before": [
                "leader",
                "m",
                "r",
                "r"
            ],
            "commands": [
                {
                    "command": "calva.refreshAll"
                }
            ],
            "when": "calva:connected"
        },
        {
            "before": [
                "leader",
                "m",
                "t",
                "p"
            ],
            "commands": [
                {
                    "command": "calva.runAllTests"
                }
            ],
            "when": "calva:connected"
        },
        {
            "before": [
                "leader",
                "m",
                "t",
                "n"
            ],
            "commands": [
                {
                    "command": "calva.runNamespaceTests"
                }
            ],
            "when": "calva:connected"
        },
        {
            "before": [
                "leader",
                "m",
                "t",
                "n"
            ],
            "commands": [
                {
                    "command": "calva.runNamespaceTests"
                }
            ],
            "when": "calva:connected"
        },
        {
            "before": [
                "leader",
                "m",
                "t",
                "t"
            ],
            "commands": [
                {
                    "command": "calva.runTestUnderCursor"
                }
            ],
            "when": "calva:connected"
        },
        //edamagit
        {
            "before": [
                "leader",
                "g",
                "g"
            ],
            "commands": [
                {
                    "command": "magit.status"
                }
            ]
        },
        {
            "before": [
                "leader",
                "g",
                "s"
            ],
            "commands": [
                {
                    "command": "magit.status"
                }
            ]
        },
        {
            "before": [
                "leader",
                "g",
                "m"
            ],
            "commands": [
                {
                    "command": "magit.dispatch"
                }
            ]
        },
        {
            "before": [
                "leader",
                "g",
                "r"
            ],
            "commands": [
                {
                    "command": "magit.refresh"
                }
            ]
        },
        {
            "before": [
                "tab"
            ],
            "commands": [
                {
                    "command": "magit.toggle-fold"
                }
            ],
            "when": "editorTextFocus && editorLangId == 'magit'"
        },
        {
            "before": [
                "leader",
                "g",
                "b"
            ],
            "commands": [
                {
                    "command": "magit.blame-file"
                }
            ],
            "when": "editorTextFocus"
        },
        {
            "before": [
                "leader",
                "g",
                "f",
                "l"
            ],
            "commands": [
                {
                    "command": "magit.log-file"
                }
            ],
            "when": "editorTextFocus"
        },
        {
            "before": [
                "leader",
                "g",
                "f",
                "d"
            ],
            "commands": [
                {
                    "command": "magit.diff-file"
                }
            ],
            "when": "editorTextFocus"
        },
        {
            "before": [
                "leader",
                "g",
                "f",
                "m"
            ],
            "commands": [
                {
                    "command": "magit.file-popup"
                }
            ],
            "when": "editorTextFocus"
        },
        {
            "before": [
                "leader",
                "g",
                "S"
            ],
            "commands": [
                {
                    "command": "magit.stage-file"
                }
            ],
            "when": "editorTextFocus"
        },
        {
            "before": [
                "leader",
                "g",
                "U"
            ],
            "commands": [
                {
                    "command": "magit.unstage-file"
                }
            ],
            "when": "editorTextFocus"
        }
    ],
    "vim.useSystemClipboard": true,
    "vim.visualModeKeyBindings": [
        {
            "before": [
                "leader",
                "m",
                "e",
                "r"
            ],
            "commands": [
                {
                    "command": "calva.evaluateSelection"
                }
            ],
            "when": "calva:keybindingsEnabled && editorTextFocus && !suggestWidgetVisible && editorLangId == 'clojure'"
        }
    ],
    "vscode-hurl-runner.verboseMode": "very-verbose",
    "window.commandCenter": false,
    "window.confirmBeforeClose": "always",
    "window.title": "${rootName}${separator}${profileName}",
    "window.zoomLevel": 1.3,
    "workbench.activityBar.location": "hidden",
    "workbench.colorCustomizations": {
        "editor.lineHighlightBackground": "#22242B",
        "editorLineNumber.foreground": "#5B6268",
        "statusBar.background": "#22242B",
        "statusBar.foreground": "#9B9C9F"
    },
    "workbench.colorTheme": "Doom Emacs",
    "workbench.editor.showTabs": "single",
    "workbench.iconTheme": "vscode-icons",
    "workbench.layoutControl.type": "menu",
    "workbench.secondarySideBar.defaultVisibility": "hidden",
    "workbench.sideBar.location": "left",
    "workbench.statusBar.visible": true,
    "workbench.tree.renderIndentGuides": "none"
}